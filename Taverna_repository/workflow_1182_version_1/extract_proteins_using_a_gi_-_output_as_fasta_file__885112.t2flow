  </inputs>
  <outputs>
    <net.sf.taverna.t2.workflowmodel.processor.activity.config.ActivityOutputPortDefinitionBean>
      <name>fastas_out</name>
      <depth>0</depth>
      <mimeTypes />
      <granularDepth>0</granularDepth>
